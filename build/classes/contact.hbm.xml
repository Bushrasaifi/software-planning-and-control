<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="Pojo.contact" table="contact">
		<id name="uid" type="int" column="uid">
			<generator class="increment"></generator>
		</id>
		<property name="uname" column="name" type="string"></property>		
		<property name="uemail" column="email" type="string"></property>
		<property name="subject" column="subject" type="string"></property>
		<property name="message" column="message" type="string"></property>
		
	</class>
</hibernate-mapping>