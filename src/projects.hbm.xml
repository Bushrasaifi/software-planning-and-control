<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="Pojo.projects" table="projects">
		<id name="pid" type="int" column="pid">
			<generator class="increment"></generator>
		</id>
		<property name="ptech" column="ptech"></property>
		<property name="pname" column="name"></property>		
		<property name="pimages" column="images"></property>
		<property name="pfiles" column="projectFiles"></property>
		
		
	
        
	</class>
</hibernate-mapping>